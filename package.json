{
  "name": "jssketches",
  "version": "1.0.0",
  "description": "list of sketches using p5.js",
  "main": "./src/index.html",
  "scripts": {
    "start": "live-server ./src/",
    "generatePalette": "npm run ffmpeg generatePalette",
    "generateGif": "npm run ffmpeg generateGif",
    "generateMp4": "npm run ffmpeg generateMp4",
    "build": "npm run ffmpeg generateAll",
    "ffmpeg": "node ./src/cmd/exec_ffmpeg.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doriclaudino/jssketches.git"
  },
  "keywords": [
    "js",
    "javascript",
    "p5js"
  ],
