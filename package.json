  "bugs": {
    "url": "https://github.com/doriclaudino/jssketchesissues"
  },
  "homepage": "https://github.com/doriclaudino/jssketches",
  "devDependencies": {
    "child_process": "^1.0.2",
    "hasbin": "^1.2.3",
    "live-server": "^1.2.1"
  }
}